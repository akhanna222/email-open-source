{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "mode": {
      "type": "string",
      "enum": ["waitAll", "passThrough", "keepOnlyFirstInput", "append"],
      "default": "waitAll",
      "description": "Merge strategy"
    },
    "clashHandling": {
      "type": "string",
      "enum": ["addSuffix", "preferInput1", "preferInput2"],
      "default": "addSuffix",
      "description": "How to handle property name conflicts"
    }
  }
}
